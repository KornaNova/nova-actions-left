{
    "name": "saintsystems/nova-actions-left",
    "version":"4.0.0",
    "description": "Laravel Nova actions on the left",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "laravel/nova": "^4.13",
        "nova-kit/nova-packages-tool": "^1.3.4"
    },
    "autoload": {
        "psr-4": {
            "SaintSystems\\Nova\\ActionsLeft\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "SaintSystems\\Nova\\ActionsLeft\\NovaActionsLeftServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
