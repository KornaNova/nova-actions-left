{
    "name": "saintsystems/nova-actions-left",
    "version":"4.0.0",
    "description": "Laravel Nova actions on the left",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "SaintSystems\\Nova\\ActionsLeft\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SaintSystems\\Nova\\ActionsLeft\\NovaActionsLeftServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
